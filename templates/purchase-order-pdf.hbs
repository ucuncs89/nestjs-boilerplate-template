<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN'
      crossorigin='anonymous'
    />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
    <link
      href='https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans&display=swap'
      rel='stylesheet'
    />
    <style>
      body { font-family: "Plus Jakarta Sans", sans-serif; } .text-low { color:
      #64748b; } @media print { .pagebreak { page-break-inside: avoid; } /*
      page-break-after works, as well */ }
    </style>

    <title>{{title}}</title>
  </head>

  <body class='p-4' id='body' onload='download()'>
    <div class='container-fluid'>
      <div class='row'>
        <div class='col-12'>
          <div class='row'>
            <div class='col-10'>
              <h3 class='mb-4'>Purchase Order Details</h3>
              <div class='d-flex gap-4'>
                <p>Number : {{code}}</p>
                <p>
                  <u><i>{{status}}</i></u>
                </p>
              </div>
            </div>
            <div class='col-2'>
              <img
                src='data:image/png;base64,'
                alt=''
              />
            </div>
          </div>
          <br /><br />
          <div class='row'>
            <div class='col-6'>
              <div class='card'>
                <div class='card-body'>
                  <p><strong>Origin of Shipment</strong></p>
                  <p>{{company.name}}</p>
                  <p>{{company.address}}</p>
                  <p>{{company.phone_number}}</p>
                </div>
              </div>
            </div>
            <div class='col-6'>
              <div class='card'>
                <div class='card-body'>
                  <p><strong>Delivery destination</strong></p>
                  <p>{{company_name}}</p>
                  <p>{{company_address}}</p>
                  <p>{{company_phone_number}}</p>
                </div>
              </div>
            </div>
          </div>
          <br /><br />
          <div class='row'>
            <div class='col-12'>
              <h5>Cost Details</h5>
              <table class='table'>
                <thead>
                  <tr>
                    <th>Number</th>
                    <th>Description</th>
                    <th>QTY</th>
                    <th>Unit</th>
                    <th>Unit price</th>
                    <th>Total</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each cost_details}}
                    <tr>
                      <td>{{incremented @index}}</td>
                      <td style='word-break: break-all'>{{description}}</td>
                      <td>{{formatCurrency price}}</td>
                      <td>{{unit}}</td>
                      <td>{{quantity}}</td>
                      <td>{{formatCurrency total_price}}</td>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
      <div class='row pagebreak' id='page-break-row'>
        <div class='col-5'>
          <div class='card'>
            <div class='card-body'>
              <p class='fw-bold fs-5'>Payment</p>
              <div>
                <div class='d-flex justify-content-between'>
                  <p>Bank name</p>
                  <p class='text-low'>{{bank_name}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>Account</p>
                  <p class='text-low'>{{bank_account_number}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>On behalf of</p>
                  <p class='text-low'>{{bank_account_houlders_name}}</p>
                </div>
              </div>
              <div class='border-top pt-3'>
                <div class='d-flex justify-content-between'>
                  <p>Payment Term</p>
                  <p class='text-low'>{{payment_term}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>Delivery Date</p>
                  <p class='text-low'>{{delivery_date}}</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class='col-7'>
          <div class='card'>
            <div class='card-body'>
              <div>
                <div class='d-flex justify-content-between'>
                  <p>Total</p>
                  <p class='text-low'>{{formatCurrency total}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>Discount</p>
                  <p class='text-low'>{{formatCurrency discount}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>PPN {{ppn}}%</p>
                  <p class='text-low'>{{formatCurrency ppn_result}}</p>
                </div>
                <div class='d-flex justify-content-between'>
                  <p>PPH {{pph}}%</p>
                  <p class='text-low'>{{formatCurrency pph_result}}</p>
                </div>
              </div>
              <div class='border-top pt-3'>
                <div class='d-flex justify-content-between'>
                  <p>Grand Total</p>
                  <p style='color: #6e7859; font-size: 24px; font-weight: 700;'>

                    {{formatCurrency grand_total}}
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js'
      integrity='sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz'
      crossorigin='anonymous'
    ></script>
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js'
      integrity='sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>
    <script>
      async function download() { var element = document.getElementById("body");
      const pdfOptions = { margin: 10, filename: "purchase-order.pdf", image: {
      type: "jpeg", quality: 0.98 }, html2canvas: { scale: 2 }, jsPDF: { unit:
      "mm", format: "a4", orientation: "potrait" }, pagebreak: { mode:
      "avoid-all", before: "#page-break-row" }, }; await
      html2pdf().from(element).set(pdfOptions).save(); return window.close(); }
    </script>
  </body>

</html>