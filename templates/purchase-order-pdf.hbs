<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC'
      crossorigin='anonymous'
    />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin='' />
    <link
      href='https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@200;400;500;600;700;800&amp;display=swap'
      rel='stylesheet'
    />
    <style>
      * { font-family: "Plus Jakarta Sans", sans-serif; } body { padding: 32px;
      } .logo { height: 32px; width: 103px; } .text-low { color: #64748b; }
      .text-brand { color: #6e7859; } @media print { .pagebreak {
      page-break-inside: avoid; } /* page-break-after works, as well */ }
    </style>
    <title>{{code}}</title>
  </head>
  <body id='body' onload='download()'>
    <div class='d-flex flex-column gap-4'>
      <img
        src='data:image/png;base64,'
        alt=''
        class='logo'
      />
      <h2 class='fs-3 fw-bold'>Purchase Order</h2>
      <div class='px-0'>
        <div class='row'>
          <div class='col-8'>
            <div class='d-flex flex-column text-low'>
              <p>Purchase Order Number : {{code}}</p>
              <p>{{dateLocalFormat created_at}}</p>
              <p>Purchase {{type}}</p>
            </div>
          </div>
          <div class='col-4'>
            <div class='d-flex flex-column text-low'>
              <p class='fw-bold'>Billed To</p>
              <p class='text-foundation-brand'>{{company_name}}</p>
              <p>{{company_address}}</p>
              <p>{{company_phone_number}}</p>
            </div>
          </div>
        </div>
      </div>
      <div class='row-cols-auto gap-6 max-width'>
        <table class='w-100'>
          <thead>
            <tr class='fw-bold text-dark'>
              <th width='{20}' class='p-4 border-end'>Number</th>
              <th class='max-width p-4 border-end'>Item</th>
              <th width='{170}' class='max-width p-4 border-end'>QTY</th>
              <th width='{170}' class='max-width p-4 border-end'>Unit</th>
              <th width='{170}' class='max-width p-4 border-end'>Unit Price</th>
              <th width='{170}' class='max-width p-4'>Total</th>
            </tr>
          </thead>
          <tbody>
            {{#each cost_details}}
              <tr class='fw-medium text-dark'>
                <td class='p-4 align-top border-top border-end'>
                  <p>{{incremented @index}}</p>
                </td>
                <td class='p-4 align-top border-top border-end'>
                  <p>{{item}}</p>
                </td>
                <td class='p-4 align-top border-top border-end'>
                  <p>{{quantity}}</p>
                </td>
                <td class='p-4 align-top border-top border-end'>
                  <p>{{unit}}</p>
                </td>
                <td class='p-4 align-top border-top border-end'>
                  <p>{{formatCurrency unit_price}}</p>
                </td>
                <td class='p-4 align-top border-top'>
                  <p>{{formatCurrency sub_total}}</p>
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <div
          class='d-flex justify-content-end fw-bold text-dark pagebreak'
          id='page-break-row'
          style='padding-top: 12px;'
        >
          <div class='row-cols-auto' style='width: 320px'>
            <div class='row'>
              <div class='col gap-2'>
                <p>Total</p>
              </div>
              <div class='col gap-2'>
                <p>{{formatCurrency total}}</p>
              </div>
            </div>
            <div class='row'>
              <div class='col gap-2'>
                <p>Discount</p>
              </div>
              <div class='col gap-2'>
                {{#if discount}}
                  <p>{{formatCurrency discount}}</p>
                {{else}}
                  <p>{{formatCurrency 0}}</p>
                {{/if}}

              </div>
            </div>
            <div class='row'>
              <div class='col gap-2'>
                {{#if ppn}}
                  <p>PPN {{ppn}} %</p>
                {{else}}
                  <p>PPN 0 %</p>
                {{/if}}
              </div>
              <div class='col gap-4'>
                <p>{{formatCurrency ppn_result}}</p>
              </div>
            </div>
            <div class='row'>
              <div class='col gap-2 border-bottom'>
                {{#if ppn}}
                  <p>PPN {{ppn}} %</p>
                {{else}}
                  <p>PPN 0 %</p>
                {{/if}}
              </div>
              <div class='col gap-2 border-bottom'>
                <p>{{formatCurrency pph_result}}</p>
              </div>
            </div>
            <div class='row py-4'>
              <div class='col gap-2'>
                <p class='fs-6 text-brand'>Grand Total</p>
              </div>
              <div class='col gap-2'>
                <p>{{formatCurrency grand_total}}</p>
              </div>
            </div>
          </div>
        </div>
        <div class='d-flex flex-column gap-3'>
          <div class='row align-items-center'>
            <p class='fs-3 fw-bold text-dark'>Thank you !</p>
            <div class='col-5'>
              <div
                class='d-flex flex-column gap-2 py-4 text-lg border-t text-low border-top'
              >
                <p class='fs-6 fw-bold text-dark'>Payment Information</p>
                <div class='d-flex justify-content-between max-width'>
                  <p class='text-dark'>Bank Name</p>
                  <p class='text-end text-low'>{{bank_name}}</p>
                </div>
                <div class='d-flex justify-content-between max-width'>
                  <p class='text-dark'>Account Number</p>
                  <p class='text-end text-low'>{{bank_account_number}}</p>
                </div>
                <div class='d-flex justify-content-between max-width'>
                  <p class='text-dark'>Account Name</p>
                  <p class='text-end text-low'>
                    {{bank_account_houlders_name}}
                  </p>
                </div>
              </div>
              <div
                class='d-flex flex-column gap-2 py-4 fs-5 border-top text-low'
              >
                <div class='d-flex justify-content-between max-width'>
                  <p class='text-dark'>Payment Term</p>
                  <p class='text-end text-low'>{{payment_term}}</p>
                </div>
                <div class='d-flex justify-content-between max-width'>
                  <p class='text-dark'>Delivery Date</p>
                  <p class='text-end text-low'>{{delivery_date}}</p>
                </div>
              </div>
            </div>
            <div class='col-7 d-flex flex-column gap-2 text-end text-low'>
              <p class='fs-3 fw-bold text-brand'>Cloami</p>
              <p>{{company.address}}</p>
              <p>{{company.phone_number}}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js'
      integrity='sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM'
      crossorigin='anonymous'
    ></script>
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js'
      integrity='sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>
    <script>
      async function download() { var element = document.getElementById("body");
      const pdfOptions = { margin: 0, filename: "{{code}}.pdf", image: { type:
      "jpeg", quality: 0.98 }, html2canvas: { scale: 2 }, jsPDF: { unit: "mm",
      format: "a4", orientation: "landscape" }, pagebreak: { mode: "avoid-all",
      before: "#page-break-row" }, }; await
      html2pdf().from(element).set(pdfOptions).save(); }
    </script>
  </body>
</html>